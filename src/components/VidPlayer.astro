---
    const {src} = Astro.props;
---

<div class="VidPlayer">
	<center>
		<iframe 
			height="400"
			width="710"
			class="iframeWrapper"
			src={src} 
			title="AlphaTree Service Player" 
			allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" 
			allowfullscreen>
		</iframe>
	</center>
</div> 